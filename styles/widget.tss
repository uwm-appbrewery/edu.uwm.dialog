	color: "#000",
	'font': {
		'fontSize': "11dp"
	},
	text: "Alert"
},
"#dialogBoxLabel": {
	opacity: 0,
	top: "16.5dp",
	right: "6.7%",
	left: "6.7%",
	'font': {
		'fontSize': "13dp"
	},
	textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
	color: "#000",
