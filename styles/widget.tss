".padding": {
	height: "16.5dp",
	width: "16.5dp"
}
"#dimmedBackground": {
	layout: "absolute",
	top: "0dp",
	height: Ti.UI.FILL,
	width: Ti.UI.FILL,
	backgroundColor: "#000",
	opacity: "0"
},
"#dialogBox": {	
	height: Ti.UI.SIZE,
	width: "250dp",
	borderRadius: "4",
	backgroundColor: "#fff",
	layout: "vertical"
},
"#dialogBoxTone1": {
	layout: "vertical",
	opacity: 0,
	top: "0dp",
	height: Ti.UI.SIZE,
	width: Ti.UI.FILL,
	backgroundColor: Alloy.Globals.settings.colors.yellow,
},
"#dialogBoxTone2": {
	layout: "vertical",
	top: "0dp",
	height: Ti.UI.SIZE,
	width: Ti.UI.FILL,
	backgroundColor: "#fff"
},
"#dialogBoxTitle": {
	opacity: 0,
	top: "16.5dp",
	right: "6.7%",
	left: "6.7%",
